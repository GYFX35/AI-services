#: app.py:300
#, python-format
msgid ""
"Error on line %(line_number)s: Invalid format. Each line must be in 'key:"
" value' format."
msgstr ""
"Error en la línea %(line_number)s: Formato no válido. Cada línea debe "
"tener el formato 'clave: valor'."

#: app.py:312
#, python-format
msgid ""
"Error on line %(line_number)s: Indentation error or item outside of a "
"section."
msgstr ""
"Error en la línea %(line_number)s: Error de sangría o elemento fuera de "
"una sección."

#: app.py:314
msgid "My Website"
msgstr "Mi sitio web"

